<SelectDialog
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	id="AddressSelectDialog"
	title="{i18n>CONTACT_ADDRESS}"
	noDataText=""
	multiSelect=""
	items="{/Countries}"
	search="searchAddress"
	confirm="closeAddressSelectDialog">
	<StandardListItem title="{Name}">
		<core:customData>
			<core:CustomData
				key="Id"
				value="Name" />
		</core:customData>
	</StandardListItem>
</SelectDialog>